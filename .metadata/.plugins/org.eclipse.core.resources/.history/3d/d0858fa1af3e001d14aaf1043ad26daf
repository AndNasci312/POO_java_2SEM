package poovetoresemlista;

public class ClasseVendedor {
			
	
		String nome;
		int id;
		
		public String getNome() {
			return nome;
		}
		public void setNome(String nome) {
			this.nome = nome;
		}
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id = id;
		}
		
		public ClasseVendedor(String nome, int id) {
			super();
			this.nome = nome;
			this.id = id;
		}
		
		
}
